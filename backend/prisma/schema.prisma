datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Registration {
  id               Int      @id @default(autoincrement())
  businessName     String
  organizationType String
  pan              String
  ownerName        String
  aadhar           String
  email            String
  pincode          String
  city             String
  state            String
  createdAt        DateTime @default(now())
}
